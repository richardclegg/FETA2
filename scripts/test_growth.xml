<FetaOptions>
    <DataFile>
        <GraphOutput>experiments/BAexperiment.dat</GraphOutput>
        <GraphInput>scripts/simple_start_graph.dat</GraphInput>
        <FormatRead>NNT</FormatRead>
        <Complex>false</Complex>
    </DataFile>
    <Action>
        <Type>Grow</Type>  
        <Start>10</Start>
    </Action>
    <OperationModel>
        <Cliques>false</Cliques>
        <Type>feta.operationmodel.PreferentialAttachment</Type>
        <MaxNodes>50000</MaxNodes>
    </OperationModel>
    <ObjectModel>
        <Entity>Link</Entity>
        <Multiply>false</Multiply>
        <Element>
            <Type>feta.objectmodel.DegreeModelElement</Type>
            <Weight>1.0</Weight>
        </Element>
    </ObjectModel>
    <ObjectModel>
        <Entity>Node</Entity>
        <Multiply>false</Multiply>
        <Element>
            <Type>feta.objectmodel.RankPreferenceElement</Type>
            <Weight>1.0</Weight>
            <Power>0.3333</Power>
        </Element>
    </ObjectModel>
</FetaOptions>
